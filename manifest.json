{
    "manifest_version": 3,
    "name": "EME Blocker",
    "version": "1.0",
    "description": "Blocks Encrypted Media Extensions (EME) API in the browser.",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],  
        "js": ["content.js"],      
        "run_at": "document_start"
      }
    ]
  }
  